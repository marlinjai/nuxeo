<?xml version="1.0"?>
<package type="addon" name="nuxeo-mariadb-package" version="@VERSION@">
  <title>MariaDB for Nuxeo</title>
  <description>
    <p>The Nuxeo Package to configure your server to use MariaDB.</p>
  </description>
  <vendor>Nuxeo</vendor>
  <target-platform>
    <name>@DISTRIBUTION_NAME@</name>
    <version>@VERSION@</version>
  </target-platform>
  <conflicts>
    <package>nuxeo-mssql-package</package>
    <package>nuxeo-mysql-package</package>
    <package>nuxeo-oracle-package</package>
  </conflicts>
